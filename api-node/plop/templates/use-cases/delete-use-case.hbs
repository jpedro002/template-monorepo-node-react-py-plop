import { {{pascalCase model}}Repository, IReqParams } from '../../repositories/{{kebabCase model}}-repository'

interface DeleteRequest {
  params: IReqParams
}

export class Delete{{pascalCase model}}UseCase {
  constructor(private {{camelCase model}}Repository: {{pascalCase model}}Repository) {}

  async execute({ params }: DeleteRequest) {
    try {
      await this.{{camelCase model}}Repository.del(params)
      
      return {
        success: true
      }
    } catch (error) {
      return {
        success: false,
        error: error instanceof Error ? error.message : 'Erro ao excluir {{camelCase model}}'
      }
    }
  }
}